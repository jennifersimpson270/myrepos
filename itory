<script>
            // Array for optimizely to push the optimizely events, needed for the AB Test
            window.optimizelyData = window.optimizelyData || []

            // Optimizely events container to map Mapp events to optimizely
            window.optimizelyEventsContainer = window.optimizelyEventsContainer || []

            window.optimizely = window.optimizely || [];
            window.optimizely.push({
                "type": "user",
                "attributes": {
                    "login_status": window._datalayer.user.loginStatus ? 'Logged in' : 'Logged out'
                }
            });
            window.optimizely.push({
                "type": "user",
                "attributes": {
                    "sst_entrypoint": window._datalayer.pageType
                }
            });
            window.optimizely.push({
                "type": "user",
                "attributes": {
                    "screen_size": window.innerWidth < 1024 ? 'Mobile view' : 'Desktop view'
                }
            });

            if (!sessionStorage.getItem('sessionCheck')) {
                sessionStorage.setItem('sessionCheck', 'newSession');

                window.optimizely.push({
                    "type": "user",
                    "attributes": {
                        "ret_visitor": localStorage.getItem('visitorCheck') ? 'Returning Visitor' : 'New Visitor'
                    }
                });
                localStorage.setItem('visitorCheck', 'true');
            }

            //Segmentation for ENGPROD-673: Get Login Status when entering basket page - This should be removed after the test has ended
            if (window._datalayer && window._datalayer.pageType === 'checkout_0_basket') {
                window.optimizely.push({
                    "type": "user",
                    "attributes": {
                        "basket_login_status": window._datalayer.user.loginStatus ? 'Basket - Logged in' : 'Basket - Logged out'
                    }
                });
            }
        </script>
